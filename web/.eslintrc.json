{
  "env": {
    "browser": true
  },
  "extends": ["plugin:react/recommended", "../.eslintrc.json"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "plugins": ["react"],
  "ignorePatterns": "**/snowpack.config.js",
  "rules": {
    "react/prop-types": "off",
    "react/display-name": "off"
  }
}
